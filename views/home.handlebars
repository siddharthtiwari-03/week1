<title>Home page</title>
<h3>
    This is home page&excl;
</h3>

<table class="table table-hover table-striped">

    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Created On
            </th>
        </tr>
    </thead>

    <tbody>

        {{#each data}}

        <tr>
            <td>
                {{this.firstName}}
            </td>
            <td>
                {{this.lastName}}
            </td>
            <td>
                {{dateFormat this.createdOn}}
            </td>
        </tr>

        {{/each}}

    </tbody>

</table>